{
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "framework": "nuxtjs",
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "continue": true
    }
  ],
  "env": {
    "NUXT_PUBLIC_SUPABASE_URL": "@supabase_url",
    "NUXT_PUBLIC_SUPABASE_KEY": "@supabase_key"
  }
}
